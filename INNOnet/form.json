{
    "elements": [
        {
            "type": "ValidationTextBox",
            "caption": "API Key",
            "name": "tb_APIKEY"
        },
        {
            "type": "ValidationTextBox",
            "caption": "Zaehlpunkt Bezug",
            "name": "tb_ZaehlpunktBezug"
        },
        {
            "type": "SelectDateTime",
            "name": "queryStart_InitTS",
            "caption": "Inital Start DateTime"
        },
        {
            "type": "ExpansionPanel",
            "caption": "Timeseries Tariff-Signal",
            "expanded": true,
            "items": [
                {
                    "type": "CheckBox",
                    "caption": "Auto Update 'Timeseries Tariff-Signal'",
                    "enabled": true,
                    "name": "cb_TariffSignal_EnableAutoUpdate"
                },           
                {
                    "type": "NumberSpinner",
                    "name": "ns_TariffSignal_UpdateInterval",
                    "caption": "Abfrage Interval [Minuten]",
                    "suffix": "Min"
                },
                {
                    "type": "NumberSpinner",
                    "name": "ns_TariffSignal_OffsetHours",
                    "caption": "Zeitspanne pro Abfrage [Stunden]",
                    "suffix": "Stunden"
                },
                {
                    "type": "CheckBox",
                    "caption": "INNOnet TariffSignal 'Value'",
                    "enabled": true,
                    "name": "cb_TariffSignal_Value"
                },
                {
                    "type": "CheckBox",
                    "caption": "INNOnet TariffSignal 'Flag",
                    "enabled": true,
                    "name": "cb_TariffSignal_Flag"
                },
                {
                    "type": "CheckBox",
                    "caption": "Sonnenfenster Infos",
                    "enabled": true,
                    "name": "cb_TariffSignal_CreateSonnenfensterInfos"
                }
            ]
        },
        {
            "type": "ExpansionPanel",
            "caption": "TimeSeriesCollections SelectedData",
            "expanded": true,
            "items": [
                {
                    "type": "CheckBox",
                    "caption": "Auto Update 'Selected Data'",
                    "enabled": true,
                    "name": "cb_SelectedData_EnableAutoUpdate"
                },
                {
                    "type": "NumberSpinner",
                    "name": "ns_SelectedData_QueryHours",
                    "caption": "Zeitspanne pro Abfrage [Stunden]",
                    "suffix": "Stunden"
                },
                {
                    "type": "NumberSpinner",
                    "name": "ns_SelectedDataQuery_OffsetDaysUntilNow",
                    "caption": "Abfrage heute minus Tage",
                    "suffix": "Tage"
                },
                {
                    "type": "CheckBox",
                    "caption": "INNOnet Tarif",
                    "enabled": true,
                    "name": "cb_SelectedData_InnonetTarif"
                },
                {
                    "type": "CheckBox",
                    "caption": "aWATTar Energie",
                    "enabled": true,
                    "name": "cb_SelectedData_aWATTarEnergy"
                },
                {
                    "type": "CheckBox",
                    "caption": "aWATTar Fee",
                    "enabled": true,
                    "name": "cb_SelectedData_aWATTarFee"
                },
                {
                    "type": "CheckBox",
                    "caption": "aWATTar VAT",
                    "enabled": true,
                    "name": "cb_SelectedData_aWATTarVat"
                },
                {
                    "type": "CheckBox",
                    "caption": "Tarif Signal",
                    "enabled": true,
                    "name": "cb_SelectedData_TariffSignal"
                },
                {
                    "type": "CheckBox",
                    "caption": "OBIS Lieferung",
                    "enabled": true,
                    "name": "cb_SelectedData_obisLieferung"
                },
                {
                    "type": "CheckBox",
                    "caption": "OBIS Bezug",
                    "enabled": true,
                    "name": "cb_SelectedData_obisBezug"
                },
                {
                    "type": "CheckBox",
                    "caption": "OBIS Gemeinschaftliche-Erzeugung",
                    "enabled": true,
                    "name": "cb_SelectedData_obisEnergyCommunity"
                }
            ]
        },
        {
            "name": "LogLevel",
            "type": "Select",
            "caption": "Debug/Log Level",
            "options": [
                {
                    "label": "ALL",
                    "value": 9
                },
                {
                    "label": "TEST",
                    "value": 8
                },
                {
                    "label": "TRACE",
                    "value": 7
                },
                {
                    "label": "COMMUNICATION",
                    "value": 6
                },
                {
                    "label": "DEBUG",
                    "value": 5
                },
                {
                    "label": "INFO",
                    "value": 4
                },
                {
                    "label": "WARN",
                    "value": 3
                },
                {
                    "label": "ERROR",
                    "value": 2
                },
                {
                    "label": "FATAL",
                    "value": 1
                },
                {
                    "label": "OFF",
                    "value": 0
                }
            ]
        }
