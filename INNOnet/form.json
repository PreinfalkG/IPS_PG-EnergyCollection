{
    "elements": [
        {
            "type": "ValidationTextBox",
            "caption": "API Key",
            "name": "tb_APIKEY"
        },
        {
            "type": "ValidationTextBox",
            "caption": "Zaehlpunkt Bezug",
            "name": "tb_ZaehlpunktBezug"
        },
        {
            "type": "SelectDateTime",
            "name": "queryStart_InitTS",
            "caption": "Inital Start DateTime"
        },
        {
            "type": "ExpansionPanel",
            "caption": "Timeseries Tariff-Signal",
            "expanded": true,
            "items": [
                {
                    "type": "CheckBox",
                    "caption": "Auto Update 'Timeseries Tariff-Signal'",
                    "enabled": true,
                    "name": "cb_TariffSignal_EnableAutoUpdate"
                },           
                {
                    "type": "NumberSpinner",
                    "name": "ns_TariffSignal_UpdateInterval",
                    "caption": "Abfrage Interval [Minuten]",
                    "suffix": "Min"
                },
                {
                    "type": "NumberSpinner",
                    "name": "ns_TariffSignal_OffsetHours",
                    "caption": "Zeitspanne pro Abfrage [Stunden]",
                    "suffix": "Stunden"
                },
                {
                    "type": "CheckBox",
                    "caption": "INNOnet TariffSignal 'Value'",
                    "enabled": true,
                    "name": "cb_TariffSignal_Value"
                },
                {
                    "type": "CheckBox",
                    "caption": "INNOnet TariffSignal 'Flag",
                    "enabled": true,
                    "name": "cb_TariffSignal_Flag"
                },
                {
                    "type": "CheckBox",
                    "caption": "Sonnenfenster Infos",
                    "enabled": true,
                    "name": "cb_TariffSignal_CreateSonnenfensterInfos"
                }
            ]
        },
        {
            "type": "ExpansionPanel",
            "caption": "TimeSeriesCollections SelectedData",
            "expanded": true,
            "items": [
                {
                    "type": "CheckBox",
                    "caption": "Auto Update 'Selected Data'",
                    "enabled": true,
                    "name": "cb_SelectedData_EnableAutoUpdate"
                },
                {
                    "type": "NumberSpinner",
                    "name": "ns_SelectedData_QueryHours",
                    "caption": "Zeitspanne pro Abfrage [Stunden]",
                    "suffix": "Stunden"
                },
                {
                    "type": "NumberSpinner",
                    "name": "ns_SelectedDataQuery_OffsetDaysUntilNow",
                    "caption": "Abfrage heute minus Tage",
                    "suffix": "Tage"
                },
                {
                    "type": "CheckBox",
                    "caption": "INNOnet Tarif",
                    "enabled": true,
                    "name": "cb_SelectedData_InnonetTarif"
                },
                {
                    "type": "CheckBox",
                    "caption": "AWTAT HLAT",
                    "enabled": true,
                    "name": "cb_SelectedData_AWTAT_tid_HLAT"
                },
                {
                    "type": "CheckBox",
                    "caption": "AWTAT HLAT Fee",
                    "enabled": true,
                    "name": "cb_SelectedData_AWTAT_tid_HLAT_Fee"
                },
                {
                    "type": "CheckBox",
                    "caption": "AWTAT HLAT Vat",
                    "enabled": true,
                    "name": "cb_SelectedData_AWTAT_tid_HLAT_Vat"
                },
                {
                    "type": "CheckBox",
                    "caption": "AWTAT HLAT SUM CALC",
                    "enabled": true,
                    "name": "cb_SelectedData_AWTAT_tid_HLAT_SUM"
                },                
                {
                    "type": "CheckBox",
                    "caption": "Tarif Signal",
                    "enabled": true,
                    "name": "cb_SelectedData_TariffSignal"
                },
                {
                    "type": "CheckBox",
                    "caption": "OBIS Lieferung (Reststrombezug Energielieferanten)",
                    "enabled": true,
                    "name": "cb_SelectedData_obisLieferung"
                },
                {
                    "type": "CheckBox",
                    "caption": "OBIS Bezug (Reststrom Einspeisung)",
                    "enabled": true,
                    "name": "cb_SelectedData_obisBezug"
                },
                {
                    "type": "CheckBox",
                    "caption": "OBIS Gemeinschaftliche-Erzeugung (Gesamt Einspeisung)",
                    "enabled": true,
                    "name": "cb_SelectedData_obisEnergyCommunity"
                },
                {
                    "type": "CheckBox",
                    "caption": "OBIS EEG CALC",
                    "enabled": true,
                    "name": "cb_SelectedData_obisEegCALC"
                },                
                { 
                    "type": "Label",
                    "caption": "Die Bezeichnung der Obis Timeseries erfolgt aus Sicht des Energielieferanten. \nDas bedeutet: Die 'Lieferung' entspricht aus Sicht des Lieferanten Ihrem Reststrombezug. Umgekehrt gilt: Wenn der Lieferant von Ihnen Energie bezieht, handelt es sich um Ihre Reststrom-Einspeisung. Die Timeserie 'obis-Gemeinschaftliche-Erzeugung' umfasst Ihren gesamten Ãœberschuss, also sowohl den EEG-Anteil als auch den Anteil, der an Reststrombezieher geliefert wird." 
                }
            ]
        },
        {
            "name": "LogLevel",
            "type": "Select",
            "caption": "Debug/Log Level",
            "options": [
                {
                    "label": "ALL",
                    "value": 9
                },
                {
                    "label": "TEST",
                    "value": 8
                },
                {
                    "label": "TRACE",
                    "value": 7
                },
                {
                    "label": "COMMUNICATION",
                    "value": 6
                },
                {
                    "label": "DEBUG",
                    "value": 5
                },
                {
                    "label": "INFO",
                    "value": 4
                },
                {
                    "label": "WARN",
                    "value": 3
                },
                {
                    "label": "ERROR",
                    "value": 2
                },
                {
                    "label": "FATAL",
                    "value": 1
                },
                {
                    "label": "OFF",
                    "value": 0
                }
            ]
        }
    ],
    "actions": [
        {
            "type": "Button",
            "label": "Update Timeseries Tariff-Signal (mit AddLoggedValue)",
            "onClick": "INNOnet_TariffSignal_TimeSeriesCollection_UPDATE($id, 'ModulForm', true);"
        },
        {
            "type": "Button",
            "label": "Update Timeseries Tariff-Signal (ohne AddLoggedValue)",
            "onClick": "INNOnet_TariffSignal_TimeSeriesCollection_UPDATE($id, 'ModulForm', false);"
        },


        {
            "type": "Button",
            "label": "SelectedData_TimeSeriesCollection_NEW (mit AddLoggedValue)",
            "onClick": "INNOnet_SelectedData_TimeSeriesCollection_NEW($id, NULL, NULL, 'ModulForm', true);"
        },

        {
            "type": "Button",
            "label": "Update Timeseriescollection-Selecteddata (mit AddLoggedValue)",
            "onClick": "INNOnet_SelectedData_TimeSeriesCollection_UPDATE($id, 'ModulForm', true);"
        },
        {
            "type": "Button",
            "label": "Update Timeseriescollection-Selecteddata (ohne AddLoggedValue)",
            "onClick": "INNOnet_SelectedData_TimeSeriesCollection_UPDATE($id, 'ModulForm', false);"
        },
        {
            "type": "Button",
            "label": "Reaggregate Variables",
            "onClick": "INNOnet_ReAggregateVariables($id, 'ModulForm');"
        },
        {
            "type": "Button",
            "label": "DELETE VariableData (from 'dateTimeQueryTS' until NOW)",
            "onClick": "INNOnet_DeleteVariableData($id, 'ModulForm');"
        },
        {
            "type": "Button",
            "caption": "Reset Counter Variables",
            "onClick": "INNOnet_ResetCounterVariables($id, 'ModulForm');"
        },
        {
            "type": "Button",
            "caption": "Get Timer Info",
            "onClick": "INNOnet_GetTimerInfo($id, 'ModulForm', true);"
        },
        {
            "type": "Button",
            "caption": "Create Media Charts",
            "onClick": "INNOnet_CreateMediaCharts($id, 'ModulForm');"
        }

    ],
    "status": [
        {
            "code": 101,
            "icon": "inactive",
            "caption": "Enable AUTO Update."
        },
        {
            "code": 102,
            "icon": "active",
            "caption": "Connection to INNOnet API has been established."
        },
        {
            "code": 200,
            "icon": "inactive",
            "caption": "Error: Could not connect to INNOnet API."
        }
    ]
}